# Configuration for collectd-rabbitmq plugin

LoadPlugin python
<Plugin python>
  LogTraces true
  Interactive false
  Import "collectd_rabbitmq.collectd_plugin"
  <Module "collectd_rabbitmq.collectd_plugin">
    Username "monitoring"
    Password "<%= @monitoring_password %>"
    Realm "RabbitMQ Management"
    Host "<%= @monitoring_host %>"
    Port "<%= @admin_port %>"
  </Module>
</Plugin>
