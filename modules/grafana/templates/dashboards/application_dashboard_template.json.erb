{
  "id": 29,
  "title": "Application dashboard - <%= @app_name %>",
  "tags": [],
  "style": "dark",
  "timezone": "browser",
  "editable": true,
  "hideControls": false,
  "sharedCrosshair": false,
  "rows": [
    <% @panel_partials.each_with_index do |row, i| %>
      <%= ',' if i > 0 %> {
        "collapse": false,
        "editable": true,
        "height": 250,
        "panels": [
          <% row.each_with_index do |panel_name, j| %>
            <%= ',' if j > 0 %> <%= scope.function_template(["grafana/dashboards/application_dashboard_panels/_#{panel_name}.json.erb"]) %>
          <% end %>
        ],
        "repeat": null,
        "repeatIteration": null,
        "repeatRowId": null,
        "showTitle": false,
        "title": "Dashboard Row",
        "titleSize": "h6"
      }
    <% end %>
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "5s",
      "10s",
      "30s",
      "1m",
      "5m",
      "15m",
      "30m",
      "1h",
      "2h",
      "1d"
    ],
    "time_options": [
      "5m",
      "15m",
      "1h",
      "6h",
      "12h",
      "24h",
      "2d",
      "7d",
      "30d"
    ]
  },
  "templating": {
    "list": []
  },
  "annotations": {
    "list": [
      {
        "datasource": "Graphite",
        "enable": true,
        "iconColor": "rgba(255, 96, 96, 1)",
        "name": "Deploys",
        "tags": "deploys <%= @app_name %>"
      },
      {
        "datasource": "Graphite",
        "enable": true,
        "iconColor": "rgb(225, 251, 0)",
        "name": "Restarts",
        "target": "substr(stats.govuk.app.<%= @app_name %>.restarts,3,5)"
      }
    ]
  },
  "refresh": "5s",
  "schemaVersion": 12,
  "version": 31,
  "links": [],
  "gnetId": null
}
